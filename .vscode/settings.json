{
    "python.defaultInterpreterPath": "e:/Proyectos/ERP/backend/venv/Scripts/python.exe",
    "python.pythonPath": "e:/Proyectos/ERP/backend/venv/Scripts/python.exe",
    "python.envFile": "${workspaceFolder}/backend/.env",
    "python.analysis.typeCheckingMode": "off",
    "python.analysis.autoImportCompletions": true,
    "python.analysis.extraPaths": [
        "e:/Proyectos/ERP/backend",
        "e:/Proyectos/ERP/backend/apps",
